<% provide(:title, 'All users') %>
<h1>All users</h1>

<ul class="users">
  <% @users.each do |user| %>
  <% unless user== current_user%>
    <li>
      	<%= link_to user.email, users_show_path(user.id) %>
     
      		<%=link_to submit_tag('+')%>
 
    </li>
  <% end %>
  <%end%>
</ul>
<%= paginate @users %>