Here We Show The List Of Notifications
<ul class="users">
<% @invitations.each do |noti| %>
	<%= noti.inspect %><hr>
	<% if noti.accepted  %>
		<%= link_to "Remove Friend", friend_request_reject_notification_index_path(user_id: noti.user_id,friend_id: noti.friend_id), class: "btn btn-primary" %>
	<% else %>
		
		<%= link_to "Accept Request", friend_request_accept_notification_index_path(user_id: noti.user_id,friend_id: noti.friend_id), class: "btn btn-primary" %>
	<% end %>
<% end %>
</ul>	